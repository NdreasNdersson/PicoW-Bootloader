__FLASH_START = 0x10000000;
__BOOTLOADER_LENGTH = 32K;
__APP_INFO_LENGTH = 256;

__FLASH_APP_START = __FLASH_START + __BOOTLOADER_LENGTH;
__APP_HASH_ADDRESS = __FLASH_APP_START;
__APP_VTOR = __FLASH_APP_START + __APP_INFO_LENGTH;
__APP_LENGTH = 2048k - __BOOTLOADER_LENGTH;

ASSERT((__BOOTLOADER_LENGTH + __APP_LENGTH) <= 2048k, "Flash partitions defined incorrectly");
